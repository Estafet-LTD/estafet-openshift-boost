node("maven") {

	currentBuild.description = "Build a library from the source, then deploy a snapshot."

	properties([
	  parameters([
		 string(name: 'REPO'),
	  ])
	])

	stage("checkout") {
		git credentialsId: "git", branch: "master", url: params.REPO
	}

	stage("deploy snapshots") {
		withMaven(mavenSettingsConfig: 'maven') {
 			sh "mvn clean deploy"
		} 
	}	

}

